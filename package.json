{
  "name": "jangle",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "node ./node_modules/mocha/bin/_mocha api/**/*.spec.js",
    "test-watch": "node ./node_modules/mocha/bin/_mocha --watch api/**/*.spec.js",
    "coverage": "node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec api/**/*.spec.js",
    "coveralls": "npm run coverage && (cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage) || (echo 'Cleaning up istanbul' && rm -rf ./coverage)",
    "dev": "node_modules/nodemon/bin/nodemon.js app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryannhg/jangle.git"
  },
  "keywords": [
    "cms"
  ],
  "author": "Ryan Haskell-Glatz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ryannhg/jangle/issues"
  },
  "homepage": "https://github.com/ryannhg/jangle#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.1",
    "express": "^4.14.0",
    "express-session": "^1.15.1",
    "mongoose": "^4.7.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pug": "^2.0.0-beta11"
  },
  "devDependencies": {
    "coveralls": "^2.11.15",
    "gulp": "^3.9.1",
    "gulp-elm": "^0.6.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nodemon": "^1.11.0"
  }
}
